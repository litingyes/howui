{
  "name": "howui",
  "version": "0.0.0",
  "packageManager": "pnpm@9.5.0",
  "description": "Web components UI library",
  "author": "liting-yes <luz.liting@gmail.com> (https://liting.ink/)",
  "license": "MIT",
  "keywords": [
    "web components",
    "UI",
    "lit"
  ],
  "scripts": {
    "release": "bumpp",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "build": "pnpm --filter components build && pnpm --filter howui build",
    "play:vue": "pnpm --filter vue dev",
    "play:react": "pnpm --filter react dev"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.22.0-beta.3",
    "@eslint-react/eslint-plugin": "^1.5.15",
    "bumpp": "^9.4.1",
    "eslint": "^9.6.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "vite": "^5.3.3",
    "vite-plugin-dts": "^3.9.1"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
